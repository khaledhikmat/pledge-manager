{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "paramEnvironment": {
            "value": {
                "name": "dev",
                "product": "pledgemanager",
                "isSignalR": true,
                "isRedis": true,
                "isRedisStateStore": true,
                "isRedisPubsub": true,
                "isCosmosDB": false,
                "isServiceBus": false
            }
        },
        "paramApps": {
            "value": [
                {
                    "shortName": "campaigns",
                    "image": "khaledhikmat/pledgemanager-be-campaigns:1.5",
