                    "isPrivateRegistry": false,
                    "registryName": "index.docker.io",
                    "registryUserName": "khaledhikmat",
                    "registryPassword": "<my-pwd>",
                    "isRedisStateStore": true,
                    "isRedisPubsub": true
                },
                {
                    "shortName": "functions",
                    "image": "khaledhikmat/pledgemanager-fe-functions:1.5",
                    "port": 6002,
                    "maxReplicas": 2,
                    "isExternalIngress": true,
                    "isPrivateRegistry": false,
                    "registryName": "index.docker.io",
